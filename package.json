{
  "name": "opstore",
  "version": "2.2.3",
  "description": "An immutable operator-based state container for JavaScript.",
  "repository": "git@github.com:mariuslundgard/opstore.git",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/es/index.d.ts",
  "scripts": {
    "build": "npm-run-all tsc-*",
    "clean": "rimraf dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write \"{src,perf/src,test}/**/*.{js,ts,tsx}\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "npm run clean",
    "precommit": "npm run build && npm run lint && npm run test",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage",
    "tsc-commonjs": "tsc -p ts/tsconfig-cjs.json",
    "tsc-module": "tsc -p ts/tsconfig-es.json"
  },
  "dependencies": {
    "segmented-property": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "coveralls": "^3.0.3",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "__jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "(\\.|/)(test|spec)\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "verbose": true
  }
}
